# AWS環境変数設定例
# このファイルを .env.aws にコピーして使用

# EC2設定
export EC2_IP="YOUR_EC2_IP_HERE"
export INSTANCE_ID="i-xxxxxxxxxxxxxxxxx"
export SSH_KEY="~/.ssh/your-key.pem"

# AWS設定
export AWS_REGION="ap-northeast-1"
export S3_BUCKET="video-message-app-backups"

# アプリケーション設定
export REPO_DIR="video-message-app"
export ENVIRONMENT="dev"

# 使い方:
# source .env.aws
# ./scripts/aws-manage.sh status