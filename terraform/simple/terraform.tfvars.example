# Terraform変数設定例
# このファイルをterraform.tfvarsにコピーして使用してください

# AWSリージョン（東京）
aws_region = "ap-northeast-1"

# EC2インスタンスタイプ
# t3.medium: メモリ4GB（最小構成）
# t3.large: メモリ8GB（推奨）
# t3.xlarge: メモリ16GB（余裕のある構成）
instance_type = "t3.large"

# アプリケーション名
app_name = "video-message-app"

# 環境名
environment = "dev"

# EC2キーペア名（事前に作成が必要）
# AWS Console > EC2 > Key Pairs で作成
key_name = "my-video-app-key"