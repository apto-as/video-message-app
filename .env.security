# SECURITY CONFIGURATION TEMPLATE
# Copy this to .env and fill in your actual values

# API Keys - NEVER commit these values
D_ID_API_KEY=your-d-id-api-key-here
DID_API_KEY=your-d-id-api-key-here

# CORS Configuration (comma-separated URLs)
CORS_ORIGINS=http://localhost:55434,https://yourdomain.com

# Service URLs (environment-aware)
VOICEVOX_BASE_URL=http://localhost:50021
OPENVOICE_API_URL=http://localhost:8001

# Security Settings
SSL_VERIFY=true
ENABLE_DEBUG_LOGGING=false

# Database and Session Security
SECRET_KEY=generate-a-secure-32-character-key-here
SESSION_SECRET=generate-another-secure-key-here

# Rate Limiting
RATE_LIMIT_ENABLED=true
RATE_LIMIT_REQUESTS_PER_MINUTE=60

# Environment
ENVIRONMENT=production